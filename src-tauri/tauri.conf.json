{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "lychee",
  "version": "0.1.0",
  "identifier": "tg.lychee.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Lychee",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/lychee32px.png",
      "icons/lychee130px.png",
      "icons/lychee.icns",
      "icons/lychee.ico"
    ]
  }
}
